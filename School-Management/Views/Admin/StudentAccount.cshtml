@model IEnumerable<School_Management.Models.StudentModel>

@{
    ViewData["Title"] = "StudentAccount";
}


<div class="section mt-3">
    <div class="bg-dark p-3 roundCorder">
        <h4>Student Account</h4>
    </div>

    <div class=" mt-4">
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.StudentID)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.StudentID)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FirstName)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.FirstName)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LastName)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.LastName)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.Email)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Gender)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.Gender)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Address)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.Address)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Grade)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.Grade)
                }
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.AcademkicYear)
            </dt>
            <dd class="col-sm-10">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.AcademkicYear)
                }
            </dd>
        </dl>
    </div>
</div>

